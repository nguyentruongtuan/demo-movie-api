    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "copy-files": "^0.1.0",
    "env-var": "^7.3.1",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.5.0",
    "express-openid-connect": "^2.16.0",
